	Title = {Design and Optimization of Circular Magnetic Structures for Lumped Inductive Power Transfer Systems},
	Volume = {26},
	Year = {2011},
	Bdsk-Url-1 = {http://dx.doi.org/10.1109/TPEL.2011.2143730}}

