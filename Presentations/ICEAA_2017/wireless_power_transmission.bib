	Keywords = {antilock braking, capacitor, current density, electric charging, electric motor, electric power system infrastructure, electric trains, electric vehicles, energy conservation, energy efficiency, {EV}, magnetic resonance, motion control technique, motor torque generation, supercapacitors, terminal voltage, time 30 s, traction control systems, two-dimensional chassis motion control, wireless power supply},
	Pages = {2930--2934},
