	Issn = {0278-0046},
	Journal = {{IEEE} Transactions on Industrial Electronics},
	Keywords = {active states, capacitors, electric impedance, impedance network, invertors, operating states, ripple factor, shoot-through states, steady-state analysis, symmetrical impedance network design, Z-source, Z-source inverter},
